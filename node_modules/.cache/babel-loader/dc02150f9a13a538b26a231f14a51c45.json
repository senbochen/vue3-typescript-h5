{"ast":null,"code":"import { computed, defineComponent, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useStore } from '@/store/index';\nimport { ActionEnum } from '@/store/modules/home/action-types';\nimport { MutationsEnum } from '@/store/modules/home/mutation-types';\nconst House = defineComponent({\n  setup() {\n    const name = ref('房子');\n    const router = useRouter();\n    const store = useStore();\n\n    const back = () => {\n      router.push({\n        path: '/'\n      });\n    };\n\n    const change = () => {\n      store.dispatch(ActionEnum.Add_Count, '这是改变的第一步');\n    };\n\n    const backChange = () => {\n      store.dispatch(ActionEnum.Add_Count, '2022');\n    };\n\n    const changeMutation = () => {\n      store.commit(MutationsEnum.ADD, '');\n    };\n\n    const count = computed(() => {\n      return store.state.home.count;\n    });\n    return {\n      name,\n      back,\n      count,\n      change,\n      backChange\n    };\n  }\n\n});\nexport default House;","map":{"version":3,"sources":["/Users/mac/Desktop/vue-template/vue3-typescript-template/src/pages/house/index.vue"],"names":[],"mappings":"AAoBA,SAAS,QAAT,EAAmB,eAAnB,EAAoC,GAApC,QAA+C,KAA/C;AACA,SAAS,SAAT,QAA0B,YAA1B;AACA,SAAS,QAAT,QAAyB,eAAzB;AACA,SAAS,UAAT,QAA2B,mCAA3B;AACA,SAAS,aAAT,QAA8B,qCAA9B;AACA,MAAM,KAAI,GAAI,eAAe,CAAC;AAC5B,EAAA,KAAI,GAAK;AACP,UAAM,IAAG,GAAI,GAAG,CAAC,IAAD,CAAhB;AACA,UAAM,MAAK,GAAI,SAAS,EAAxB;AACA,UAAM,KAAI,GAAI,QAAQ,EAAtB;;AACA,UAAM,IAAG,GAAI,MAAM;AACjB,MAAA,MAAM,CAAC,IAAP,CAAY;AACV,QAAA,IAAI,EAAE;AADI,OAAZ;AAGF,KAJA;;AAKA,UAAM,MAAK,GAAI,MAAM;AACnB,MAAA,KAAK,CAAC,QAAN,CAAe,UAAU,CAAC,SAA1B,EAAqC,UAArC;AACF,KAFA;;AAIA,UAAM,UAAS,GAAI,MAAM;AACvB,MAAA,KAAK,CAAC,QAAN,CAAe,UAAU,CAAC,SAA1B,EAAqC,MAArC;AACF,KAFA;;AAIA,UAAM,cAAa,GAAI,MAAM;AAC3B,MAAA,KAAK,CAAC,MAAN,CAAa,aAAa,CAAC,GAA3B,EAAgC,EAAhC;AACF,KAFA;;AAGA,UAAM,KAAI,GAAI,QAAQ,CAAC,MAAM;AAC3B,aAAO,KAAK,CAAC,KAAN,CAAY,IAAZ,CAAiB,KAAxB;AACD,KAFqB,CAAtB;AAGA,WAAO;AACL,MAAA,IADK;AAEL,MAAA,IAFK;AAGL,MAAA,KAHK;AAIL,MAAA,MAJK;AAKL,MAAA;AALK,KAAP;AAOF;;AA/B4B,CAAD,CAA7B;AAiCA,eAAe,KAAf","sourcesContent":["\n<template>\n  <div class=\"warp\">\n    <p>{{ count }}</p>\n    <van-button type=\"primary\" @click=\"changeMutation\" style=\"width: 100%\"\n      >Vuex Mutation事件 改变\n    </van-button>\n    <van-button type=\"primary\" @click=\"change\" style=\"width: 100%\"\n      >Vuex Action事件 改变\n    </van-button>\n    <van-button type=\"success\" @click=\"backChange\" style=\"width: 100%\"\n      >Vuex Action事件 恢复</van-button\n    >\n    <van-button type=\"primary\" @click=\"back\" style=\"width: 100%\"\n      >回去</van-button\n    >\n  </div>\n</template>\n\n<script>\nimport { computed, defineComponent, ref } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { useStore } from '@/store/index'\nimport { ActionEnum } from '@/store/modules/home/action-types'\nimport { MutationsEnum } from '@/store/modules/home/mutation-types'\nconst House = defineComponent({\n  setup () {\n    const name = ref('房子')\n    const router = useRouter()\n    const store = useStore()\n    const back = () => {\n      router.push({\n        path: '/'\n      })\n    }\n    const change = () => {\n      store.dispatch(ActionEnum.Add_Count, '这是改变的第一步')\n    }\n\n    const backChange = () => {\n      store.dispatch(ActionEnum.Add_Count, '2022')\n    }\n\n    const changeMutation = () => {\n      store.commit(MutationsEnum.ADD, '')\n    }\n    const count = computed(() => {\n      return store.state.home.count\n    })\n    return {\n      name,\n      back,\n      count,\n      change,\n      backChange\n    }\n  }\n})\nexport default House\n</script>\n<style scoped lang='scss'>\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}