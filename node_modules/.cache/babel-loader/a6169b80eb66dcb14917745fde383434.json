{"ast":null,"code":"import { defineComponent, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useStore } from '@/store/index';\nimport { actionEnum } from '@/store/modules/home/action-types';\nconst House = defineComponent({\n  setup() {\n    const name = ref('房子');\n    const router = useRouter();\n    const store = useStore();\n    const count = ref(store.state.home.count);\n\n    const back = () => {\n      router.push({\n        path: '/'\n      });\n    };\n\n    const change = () => {\n      store.dispatch(actionEnum.Add_Count, '这是改变的第一步');\n      console.log(store.state.home);\n    };\n\n    return {\n      name,\n      back,\n      count,\n      change\n    };\n  }\n\n});\nexport default House;","map":{"version":3,"sources":["/Users/mac/Desktop/vue-template/vue3-typescript-template/src/pages/house/index.vue"],"names":[],"mappings":"AAWA,SAAS,eAAT,EAA0B,GAA1B,QAAqC,KAArC;AACA,SAAS,SAAT,QAA0B,YAA1B;AACA,SAAS,QAAT,QAAyB,eAAzB;AACA,SAAS,UAAT,QAA2B,mCAA3B;AACA,MAAM,KAAI,GAAI,eAAe,CAAC;AAC5B,EAAA,KAAI,GAAK;AACP,UAAM,IAAG,GAAI,GAAG,CAAC,IAAD,CAAhB;AACA,UAAM,MAAK,GAAI,SAAS,EAAxB;AACA,UAAM,KAAI,GAAI,QAAQ,EAAtB;AACA,UAAM,KAAI,GAAI,GAAG,CAAC,KAAK,CAAC,KAAN,CAAY,IAAZ,CAAiB,KAAlB,CAAjB;;AACA,UAAM,IAAG,GAAI,MAAM;AACjB,MAAA,MAAM,CAAC,IAAP,CAAY;AACV,QAAA,IAAI,EAAE;AADI,OAAZ;AAGF,KAJA;;AAKA,UAAM,MAAK,GAAI,MAAM;AAEnB,MAAA,KAAK,CAAC,QAAN,CAAe,UAAU,CAAC,SAA1B,EAAqC,UAArC;AACA,MAAA,OAAO,CAAC,GAAR,CAAY,KAAK,CAAC,KAAN,CAAY,IAAxB;AACF,KAJA;;AAKA,WAAO;AACL,MAAA,IADK;AAEL,MAAA,IAFK;AAGL,MAAA,KAHK;AAIL,MAAA;AAJK,KAAP;AAMF;;AAtB4B,CAAD,CAA7B;AAwBA,eAAe,KAAf","sourcesContent":["\n<template>\n  <div class=\"warp\">\n    <p>{{ name }}</p>\n    <p>{{ count }}</p>\n    <van-button type=\"primary\" @click=\"change\">Vuex Action事件 </van-button>\n    <van-button type=\"primary\" @click=\"back\">回去</van-button>\n  </div>\n</template>\n\n<script>\nimport { defineComponent, ref } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { useStore } from '@/store/index'\nimp