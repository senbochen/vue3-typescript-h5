{"ast":null,"code":"import { computed, defineComponent, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useStore } from '@/store/index';\nimport { ActionEnum } from '@/store/modules/home/action-types';\nimport { MutationsEnum } from '@/store/modules/home/mutation-types';\nconst House = defineComponent({\n  setup() {\n    const name = ref('房子');\n    const number = ref(0);\n    const router = useRouter();\n    const store = useStore();\n\n    const back = () => {\n      router.push({\n        path: '/'\n      });\n    };\n\n    const change = () => {\n      store.dispatch(ActionEnum.Add_Count, '这是改变的第一步');\n    };\n\n    const backChange = () => {\n      store.dispatch(ActionEnum.Add_Count, '2022');\n    };\n\n    const changeMutation = () => {\n      store.commit(MutationsEnum.NAME, {\n        name: '今年是2021年'\n      });\n    };\n\n    const count = computed(() => {\n      return store.state.home.count;\n    });\n    const contryName = computed(() => {\n      return store.state.home.name;\n    });\n    return {\n      name,\n      back,\n      count,\n      contryName,\n      change,\n      backChange,\n      changeMutation,\n      number\n    };\n  }\n\n});\nexport default House;","map":{"version":3,"sources":["/Users/mac/Desktop/vue-template/vue3-typescript-template/src/pages/house/index.vue"],"names":[],"mappings":"AAqCA,SAAS,QAAT,EAAmB,eAAnB,EAAoC,GAApC,QAA+C,KAA/C;AACA,SAAS,SAAT,QAA0B,YAA1B;AACA,SAAS,QAAT,QAAyB,eAAzB;AACA,SAAS,UAAT,QAA2B,mCAA3B;AACA,SAAS,aAAT,QAA8B,qCAA9B;AACA,MAAM,KAAI,GAAI,eAAe,CAAC;AAC5B,EAAA,KAAI,GAAK;AACP,UAAM,IAAG,GAAI,GAAG,CAAC,IAAD,CAAhB;AACA,UAAM,MAAK,GAAI,GAAG,CAAC,CAAD,CAAlB;AACA,UAAM,MAAK,GAAI,SAAS,EAAxB;AACA,UAAM,KAAI,GAAI,QAAQ,EAAtB;;AACA,UAAM,IAAG,GAAI,MAAM;AACjB,MAAA,MAAM,CAAC,IAAP,CAAY;AACV,QAAA,IAAI,EAAE;AADI,OAAZ;AAGF,KAJA;;AAKA,UAAM,MAAK,GAAI,MAAM;AACnB,MAAA,KAAK,CAAC,QAAN,CAAe,UAAU,CAAC,SAA1B,EAAqC,UAArC;AACF,KAFA;;AAIA,UAAM,UAAS,GAAI,MAAM;AACvB,MAAA,KAAK,CAAC,QAAN,CAAe,UAAU,CAAC,SAA1B,EAAqC,MAArC;AACF,