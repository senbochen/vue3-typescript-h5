{"ast":null,"code":"import { defineComponent, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useStore } from '@/store/index';\nimport { mapActions } from 'vuex';\nconst House = defineComponent({\n  setup() {\n    const name = ref('房子');\n    const router = useRouter();\n    const store = useStore();\n    const count = store.state.home.count;\n\n    const back = () => {\n      router.push({\n        path: '/'\n      });\n    };\n\n    const change = () => {\n      console.log(mapActions(['Add_Count']).Add_Count(212)); // mapActions(['Add_Count']).Add_Count('即将改变')\n    };\n\n    return {\n      name,\n      back,\n      count,\n      change\n    };\n  }\n\n});\nexport default House;","map":{"version":3,"sources":["/Users/mac/Desktop/vue-template/vue3-typescript-template/src/pages/house/index.vue"],"names":[],"mappings":"AAWA,SAAS,eAAT,EAA0B,GAA1B,QAAqC,KAArC;AACA,SAAS,SAAT,QAA0B,YAA1B;AACA,SAAS,QAAT,QAAyB,eAAzB;AACA,SAAS,UAAT,QAA2B,MAA3B;AACA,MAAM,KAAI,GAAI,eAAe,CAAC;AAC5B,EAAA,KAAI,GAAK;AACP,UAAM,IAAG,GAAI,GAAG,CAAC,IAAD,CAAhB;AACA,UAAM,MAAK,GAAI,SAAS,EAAxB;AACA,UAAM,KAAI,GAAI,QAAQ,EAAtB;AACA,UAAM,KAAI,GAAI,KAAK,CAAC,KAAN,CAAY,IAAZ,CAAiB,KAA/B;;AACA,UAAM,IAAG,GAAI,MAAM;AACjB,MAAA,MAAM,CAAC,IAAP,CAAY;AACV,QAAA,IAAI,EAAE;AADI,OAAZ;AAGF,KAJA;;AAKA,UAAM,MAAK,GAAI,MAAM;AACnB,MAAA,OAAO,CAAC,GAAR,CAAY,UAAU,CAAC,CAAC,WAAD,CAAD,CAAV,CAA0B,SAA1B,CAAoC,GAApC,CAAZ,EADmB,CAEnB;AACF,KAHA;;AAIA,WAAO;AACL,MAAA,IADK;AAEL,MAAA,IAFK;AAGL,MAAA,KAHK;AAIL,MAAA;AAJK,KAAP;AAMF;;AArB4B,CAAD,CAA7B;AAuBA,eAAe,KAAf","sourcesContent":["\n<template>\n  <div class=\"warp\">\n    <p>{{ name }}</p>\n    <p>{{ count }}</p>\n    <van-button type=\"primary\" @click=\"change\">Vuex Action事件 </van-button>\n    <van-button type=\"primary\" @click=\"back\">回去</van-button>\n  </div>\n</template>\n\n<script>\nimport { defineComponent, ref } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { useStore } from '@/store/index'\nimport { mapActions } from 'vuex'\nconst House = defi