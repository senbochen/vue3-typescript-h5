{"ast":null,"code":"import { defineComponent, reactive } from 'vue';\nimport { useStore } from '@/store/index';\nimport { MutationsEnum } from '@/store/mutation-types';\nconst Login = defineComponent({\n  name: 'Login',\n\n  setup() {\n    const state = reactive({\n      username: '',\n      password: ''\n    });\n    const store = useStore();\n\n    const onSubmit = () => {\n      store.commit(MutationsEnum.AddToken, state.username);\n    };\n\n    return {\n      state,\n      onSubmit\n    };\n  }\n\n});\nexport default Login;","map":{"version":3,"sources":["/Users/mac/Desktop/vue-template/vue3-typescript-template/src/views/login/index.vue"],"names":[],"mappings":"AAsCA,SAAS,eAAT,EAA0B,QAA1B,QAA0C,KAA1C;AACA,SAAS,QAAT,QAAyB,eAAzB;AACA,SAAS,aAAT,QAA8B,wBAA9B;AACA,MAAM,KAAI,GAAI,eAAe,CAAC;AAC5B,EAAA,IAAI,EAAE,OADsB;;AAE5B,EAAA,KAAI,GAAK;AACP,UAAM,KAAI,GAAI,QAAQ,CAAC;AACrB,MAAA,QAAQ,EAAE,EADW;AAErB,MAAA,QAAQ,EAAE;AAFW,KAAD,CAAtB;AAIA,UAAM,KAAI,GAAI,QAAQ,EAAtB;;AACA,UAAM,QAAO,GAAI,MAAM;AACrB,MAAA,KAAK,CAAC,MAAN,CAAa,aAAa,CAAC,QAA3B,EAAqC,KAAK,CAAC,QAA3C;AACF,KAFA;;AAIA,WAAO;AACL,MAAA,KADK;AAEL,MAAA;AAFK,KAAP;AAIF;;AAhB4B,CAAD,CAA7B;AAkBA,eAAe,KAAf","sourcesContent":["<!--\n * @Author: your name\n * @Date: 2021-08-13 15:49:09\n * @LastEditTime: 2021-08-13 17:05:52\n * @LastEditors: Please set LastEditors\n * @Description: In User Settings Edit\n * @FilePath: /vue3-typescript-template/src/views/login/index.vue\n-->\n<template>\n  <div class=\"login-wrap\">\n    <van-form @submit=\"onSubmit\">\n      <van-cell-group inset>\n        <van-field\n          v-model=\"state.username\"\n          name=\"用户名\"\n          label=\"用户名\"\n          placeholder=\"用户名\"\n          :rules=\"[{ required: true, message: '请填写用户名' }]\"\n        />\n        <van-field\n          v-model=\"state.password\"\n          type=\"password\"\n          name=\"密码\"\n          label=\"密码\"\n          placeholder=\"密码\"\n          :rules=\"[{ required: true, message: '请填写密码' }]\"\n        />\n      </van-cell-group>\n      <div style=\"margin: 16px\">\n        <van-button round block type=\"primary\" native-type=\"submit\">\n          提交\n        </van-button>\n      </div>\n    </van-form>\n  </div>\n</template>\n\n<script>\nimport { defineComponent, reactive } from 'vue'\nimport { useStore } from '@/store/index'\nimport { MutationsEnum } from '@/store/mutation-types'\nconst Login = defineComponent({\n  name: 'Login',\n  setup () {\n    const state = reactive({\n      username: '',\n      password: '',\n    })\n    const store = useStore()\n    const onSubmit = () => {\n      store.commit(MutationsEnum.AddToken, state.username)\n    }\n\n    return {\n      state,\n      onSubmit,\n    }\n  }\n})\nexport default Login\n\n</script>\n<style scoped lang='scss'>\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}