AAC,CAAC;EAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAC3B","file":"/Users/mac/Desktop/vue-template/vue3-typescript-template/src/pages/home/index.vue","sourceRoot":"","sourcesContent":["\n<template>\n  <div>\n    <van-swipe class=\"my-swipe\" :autoplay=\"3000\" indicator-color=\"white\">\n      <van-swipe-item\n        v-for=\"item in inforData\"\n        :key=\"item.number\"\n        style=\"display: flex; justify-content: center; flex-direction: column\"\n      >\n        <van-image width=\"100\" height=\"100\" :src=\"item.poster\" round=\"true\" />\n        <p>第{{ item.number }}金</p>\n        <p>{{ item.subTitle }}</p>\n        <p>时间：{{ item.date }}</p>\n      </van-swipe-item>\n    </van-swipe>\n    <van-button type=\"primary\" @click=\"skip\">跳转路由</van-button>\n  </div>\n</template>\n\n<script lang='ts'>\nimport { defineComponent, ref, reactive, onMounted } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { Name } from './index'\nimport { getBaseInfor } from '@/api/index'\nconst Home = defineComponent({\n  setup() {\n    const count = ref(0)\n    const inforData = ref([])\n    const house = reactive<Name>({\n      name: '房子',\n      age: 212\n    })\n    const router = useRouter()\n    const getInfor = async () => {\n      try {\n        const {\n          data: {\n            data: { dataList }\n          }\n        } = await getBaseInfor({\n          srcid: 50522,\n          full_column_event: 'tokyo_olympic_2021',\n          query: '中国金牌'\n        })\n        inforData.value = dataList\n      } catch (error) {\n        console.log(error)\n      }\n    }\n\n    //接口请求\n    onMounted(() => {\n      getInfor()\n    })\n\n    //路由跳转\n    const skip = () => {\n      router.push({ path: '/house' })\n    }\n\n    return {\n      count,\n      skip,\n      house,\n      inforData\n    }\n  }\n})\n\nexport default Home\n</script>\n<style scoped lang=\"scss\">\n.my-swipe .van-swipe-item {\n  color: #fff;\n  font-size: 20px;\n  line-height: 150px;\n  text-align: center;\n  background-color: #39a9ed;\n}\n</style>\n"]}]}